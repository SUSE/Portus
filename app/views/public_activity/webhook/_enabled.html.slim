li
  .activitie-container
    .activity-type.change-webhook-visibility
      i.fa.fa-unlock
    .user-image
      = user_image_tag(activity.owner)
    .description
      h6
        strong
          = "#{activity_owner(activity)} set the "
        - if activity.trackable.nil?
          = activity.parameters[:webhook_host]
        - else
          = link_to activity.trackable.host, [activity.trackable.namespace, activity.trackable]
        |  webhook as enabled
      small
        i.fa.fa-clock-o
        = activity_time_tag activity.created_at
